{
	"name": "webpack-dependencie",
	"description": "",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/talltotal/webpack-dependencie.git"
	},
	"keywords": [
		"webpack",
		"Dependencie"
	],
	"scripts": {
		"webpack:watch": "webpack --config=./example/webpack.conf.js --watch",
